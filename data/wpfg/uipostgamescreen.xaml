                                        <TextBlock x:Name="game_Diplomacy" Grid.Row="10" Grid.Column="1" Style="{StaticResource RightInfo}" Visibility="Visible">OFF</TextBlock>
                                        <TextBlock x:Name="game_AlliedConquest" Grid.Row="11" Grid.Column="1" Style="{StaticResource RightInfo}" Visibility="Visible">OFF</TextBlock>
                                        <TextBlock x:Name="game_EconomyPackages" Grid.Row="12" Grid.Column="1" Style="{StaticResource RightInfo}" Visibility="Visible">OFF</TextBlock>
