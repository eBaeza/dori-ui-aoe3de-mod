<Window 
    x:Class="aoe3wpfg.uipostgamescreen" 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:aoe3wpfg" 
    mc:Ignorable="d" 
    Title="uipostgamescreen" 
    x:Name="postgame_screen" Style="{StaticResource WindowStyle}">
    <Window.Resources>
        <Style x:Key="{x:Type local:Age3KeyboardFocusBorder}" TargetType="local:Age3KeyboardFocusBorder">
            <Style.Setters>
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="local:Age3KeyboardFocusBorder">
                            <Grid>
                                <Border Name="FocusBorder" Margin="0,-5,0,-5" BorderThickness="2" CornerRadius="5" BorderBrush="NavajoWhite" Grid.RowSpan="3"/>
                                <ContentPresenter Name="PART_Content"/>
                            </Grid>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style.Setters>
        </Style>

        <Style TargetType="{x:Type TabItem}" x:Key="endGameText">
            <Setter Property="FontFamily" Value="Trajan Pro" />
            <Setter Property="Height" Value="35" />
            <Setter Property="FontSize" Value="18" />
            <Setter Property="Foreground" Value="{StaticResource ButtonTextGradient}" />
            <Setter Property="Width" Value="250" />
            <Setter Property="Height" Value="50" />
            <Setter Property="VerticalContentAlignment" Value="Center" />
            <Setter Property="HorizontalContentAlignment" Value="Center" />

            <Setter Property="Effect">
                <Setter.Value>
                    <DropShadowEffect BlurRadius="10" Color="Black" Direction="-90" ShadowDepth="1.5" RenderingBias="Quality" />
                </Setter.Value>
            </Setter>

            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type TabItem}">
                        <Grid>
                            <Border Name="Border" Margin="10,0,0,0" Background="Transparent" BorderThickness="2" CornerRadius="4">
                                <ContentPresenter x:Name="ContentSite" VerticalAlignment="Center" HorizontalAlignment="Center" ContentSource="Header" Margin="12,2,12,2" RecognizesAccessKey="True">
                                </ContentPresenter>
                                <Border.BorderBrush>
                                    <LinearGradientBrush StartPoint="0,0.5" EndPoint="1,0.5">
                                        <LinearGradientBrush.GradientStops>
                                            <GradientStopCollection>
                                                <GradientStop Color="#4f4731" Offset="0.0" />
                                                <GradientStop Color="#b79f66" Offset="0.5" />
                                                <GradientStop Color="#4f4731" Offset="1.0" />
                                            </GradientStopCollection>
                                        </LinearGradientBrush.GradientStops>
                                    </LinearGradientBrush>
                                </Border.BorderBrush>
                            </Border>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsSelected" Value="True">
                                <Setter Property="Panel.ZIndex" Value="100" />
                                <Setter TargetName="Border" Property="Background" Value="#5c441d" />
                                <Trigger.EnterActions>
                                    <SoundPlayerAction Source="pack://siteoforigin:,,,/resources/audio/smallbutton.wav" />
                                </Trigger.EnterActions>
                            </Trigger>
                            <Trigger Property="IsSelected" Value="False">
                                <Setter Property="Panel.ZIndex" Value="-100" />
                                <Setter TargetName="Border" Property="Background" Value="#36270f" />
                            </Trigger>
                            <Trigger Property="IsEnabled" Value="False">
                                <Setter TargetName="Border" Property="Background" Value="DarkRed" />
                                <Setter TargetName="Border" Property="BorderBrush" Value="Black" />
                                <Setter Property="Foreground" Value="DarkGray" />
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style TargetType="TextBlock" x:Key="endGameTextStyle2">
            <Setter Property="FontFamily" Value="Trajan Pro" />
            <Setter Property="FontWeight" Value="Bold" />
            <Setter Property="FontSize" Value="21" />
            <Setter Property="Foreground" Value="White" />
            <Setter Property="TextAlignment" Value="Center" />
            <Setter Property="VerticalAlignment" Value="Center" />
            <!--<Setter Property="Padding" Value="20" />-->
            <Setter Property="Effect">
                <Setter.Value>
                    <DropShadowEffect BlurRadius="10" Color="Black" Direction="-90" ShadowDepth="2" RenderingBias="Quality" />
                </Setter.Value>
            </Setter>
        </Style>
        <Style TargetType="TextBlock" x:Key="endGameTextStyle1">
            <Setter Property="FontFamily" Value="Trajan Pro" />
            <Setter Property="FontSize" Value="18" />
            <Setter Property="Foreground" Value="White" />
            <Setter Property="FontWeight" Value="Bold" />
            <Setter Property="TextAlignment" Value="Left" />
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="TextWrapping" Value="Wrap" />
            <Setter Property="Width" Value="400"/>
            <Setter Property="Effect">
                <Setter.Value>
                    <DropShadowEffect BlurRadius="10" Color="Black" Direction="-90" ShadowDepth="2" RenderingBias="Quality" />
                </Setter.Value>
            </Setter>
        </Style>

        <Style TargetType="TextBlock" x:Key="RegularArial">
            <Setter Property="FontFamily" Value="Arial" />
            <Setter Property="FontSize" Value="22" />
            <Setter Property="Foreground" Value="White" />
            <Setter Property="HorizontalAlignment" Value="Center" />
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="TextAlignment" Value="Center" />
            <Setter Property="Width" Value="550" />
            <Setter Property="TextWrapping" Value="Wrap" />
            <Setter Property="Effect">
                <Setter.Value>
                    <DropShadowEffect BlurRadius="3" Color="Black" Direction="-90" ShadowDepth="3" RenderingBias="Quality" />
                </Setter.Value>
            </Setter>
        </Style>

        <Style TargetType="TextBlock" x:Key="AwardsTitle">
            <Setter Property="FontFamily" Value="Trajan Pro" />
            <Setter Property="FontSize" Value="22" />
            <Setter Property="FontWeight" Value="Bold" />
            <Setter Property="Foreground" Value="{StaticResource ButtonTextGradient}" />
            <Setter Property="TextAlignment" Value="Left"/>
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="TextWrapping" Value="Wrap" />
            <Setter Property="Width" Value="400"/>
            <Setter Property="Effect">
                <Setter.Value>
                    <DropShadowEffect BlurRadius="4" Color="Black" Direction="-90" ShadowDepth="4" RenderingBias="Quality" />
                </Setter.Value>
            </Setter>
        </Style>

        <BitmapImage x:Key="postgame_awards_fame" UriSource="pack://siteoforigin:,,,/resources/images/icons/post_game/fame.png" />
        <BitmapImage x:Key="postgame_awards_most_treasures" UriSource="pack://siteoforigin:,,,/resources/images/icons/post_game/awards_most_treasures.png" />
        <BitmapImage x:Key="postgame_awards_most_tech" UriSource="pack://siteoforigin:,,,/resources/images/icons/post_game/awards_most_tech.png" />
        <BitmapImage x:Key="postgame_awards_most_resources" UriSource="pack://siteoforigin:,,,/resources/images/icons/post_game/awards_most_resources.png" />
        <BitmapImage x:Key="postgame_awards_most_losses" UriSource="pack://siteoforigin:,,,/resources/images/icons/post_game/awards_most_losses.png" />
        <BitmapImage x:Key="postgame_awards_most_kills" UriSource="pack://siteoforigin:,,,/resources/images/icons/post_game/awards_most_kills.png" />
        <BitmapImage x:Key="postgame_awards_least_resources" UriSource="pack://siteoforigin:,,,/resources/images/icons/post_game/awards_least_resources.png" />
        <BitmapImage x:Key="postgame_awards_largest_army" UriSource="pack://siteoforigin:,,,/resources/images/icons/post_game/awards_largest_army.png" />
        <BitmapImage x:Key="postgame_flag_dutch" UriSource="pack://siteoforigin:,,,/resources/images/icons/flags/postgame_flag_dutch.png" />
        <BitmapImage x:Key="postgame_flag_colombian" UriSource="pack://siteoforigin:,,,/resources/images/icons/flags/postgame_flag_colombian.png" />
        <BitmapImage x:Key="postgame_flag_chinese" UriSource="pack://siteoforigin:,,,/resources/images/icons/flags/postgame_flag_chinese.png" />
        <BitmapImage x:Key="postgame_flag_chilean" UriSource="pack://siteoforigin:,,,/resources/images/icons/flags/postgame_flag_chilean.png" />
        <BitmapImage x:Key="postgame_flag_brazilian" UriSource="pack://siteoforigin:,,,/resources/images/icons/flags/postgame_flag_brazilian.png" />
        <BitmapImage x:Key="postgame_flag_aztec" UriSource="pack://siteoforigin:,,,/resources/images/icons/flags/postgame_flag_aztec.png" />
        <BitmapImage x:Key="postgame_flag_argentinian" UriSource="pack://siteoforigin:,,,/resources/images/icons/flags/postgame_flag_argentinian.png" />
        <ImageBrush x:Key="button_homecity_hover" ImageSource="pack://siteoforigin:,,,/resources/button_homecity_hover.png" Stretch="Uniform" />
        <ImageBrush x:Key="button_homecity_normal" ImageSource="pack://siteoforigin:,,,/resources/button_homecity_normal.png" Stretch="Uniform" />

        <!--divider that sits ontop of those multiplayerButtons-->
        <ControlTemplate x:Key="titleDivider">
            <Grid Height="50" VerticalAlignment="Top">
                <Rectangle Height="2" Fill="{StaticResource ButtonStrokeGradient}" VerticalAlignment="Top" Margin="0,0,0,1"></Rectangle>
            </Grid>
        </ControlTemplate>
        
        <local:Age3PopoverPage x:Key="ChatPageData" FrameUri="uichatbrowser.xaml" 
                                   IsEnabled="{Binding EnabledWhenOnlineForBeta, Source={StaticResource uiSettings}}"
                                   Icon="{StaticResource multiplayer_quickicon_chat}" 
                                   DescriptionId="70704"
                                   NotificationCount="0"/>


    </Window.Resources>

    <Viewbox Style="{StaticResource ViewboxMain}">
        <Grid Width="1920" Height="1080" Background="#66000000">
            <Grid.RowDefinitions>
                <RowDefinition Height="50"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <!--Top Bar-->
            <Grid Grid.Row="0">
                <ContentControl Template="{StaticResource Titlebar}" Visibility="Hidden" />
                <Grid Height="50" VerticalAlignment="Top">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <!--Back Button-->
                    <Grid Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Left">
                        <local:Age3Button x:Name="postgame_MainMenuButton" Style="{StaticResource SubMenuBackButton}" VerticalAlignment="Center" ConsoleCommand="leaveGame" Content="$$18445$$Quit">
                            <Button.ToolTip>$$20030$$</Button.ToolTip>
                        </local:Age3Button>
                    </Grid>

                    <StackPanel Grid.Column="0" Grid.ColumnSpan="3" Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
                        <local:Age3Button x:Name="postgame_ViewMapButton" ConsoleCommand="showGameFromPostGame" Style="{StaticResource MainMenuButtonClean}" Width="350">
                            <Button.Content>
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
                                    <TextBlock MaxWidth="300" TextAlignment="Center" TextTrimming="CharacterEllipsis" Text="$$19029$$View Map"/>
                                    <Image Source="pack://siteoforigin:,,,/resources/button_homecity_normal.png" Height="20" Margin="5,0,0,0"/>
                                                </StackPanel>
                            </Button.Content>
                        <Button.ToolTip>$$20932$$Click this button to view the map that you just played on.</Button.ToolTip>
                    </local:Age3Button>
                        <local:Age3Button x:Name="postgame_RematchButton" Style="{StaticResource MainMenuButtonClean}" Width="350" Margin="10,0,0,0" Content="$$49268$$Rematch" ConsoleCommand="rematch()">
                            <Button.ToolTip>$$49269$$Host another game with the same game parameters. Other players must be invited again to join.</Button.ToolTip>
                        </local:Age3Button>
                        <ToggleButton x:Name="postgame_ChatButton" Style="{StaticResource toggleButtons}" Width="350" Margin="10,0,0,0" Height="40" IsChecked="{Binding IsOpen, Source={StaticResource ChatPageData}, Mode=TwoWay}">
                            <ToggleButton.Content>
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock MaxWidth="300" TextTrimming="CharacterEllipsis" Text="$$20016$$Chat" Foreground="{StaticResource ButtonTextGradient}" VerticalAlignment="Center"/>
                                    <Image Source="pack://siteoforigin:,,,/resources/images/multiplayer/quickicon_chat.png" Height="25" Margin="5,0,0,0"/>
                                </StackPanel>
                            </ToggleButton.Content>
                            <ToggleButton.ToolTip>$$70704$$Click to open chat.</ToggleButton.ToolTip>
                        </ToggleButton>
                    </StackPanel>
                </Grid>
                <Grid.Effect>
                    <DropShadowEffect BlurRadius="10" Color="Black" Direction="-90" ShadowDepth="5" RenderingBias="Quality"/>
                </Grid.Effect>
            </Grid>

            <!--Post Game-->
            <Grid x:Name="ContainerGrid"  Grid.Row="1" Width="1880" VerticalAlignment="Stretch" Margin="0,10,0,0">
                <local:Age3KeyboardFocusBorder Margin="10,0,10,0">
                    <TabControl x:Name="PostgameTabControl" Grid.Column="1" Grid.Row="1" Background="Transparent" BorderBrush="Transparent" BorderThickness="0">
                        <TabControl.Resources>
                            <Style TargetType="{x:Type TabPanel}">
                                <Setter Property="HorizontalAlignment" Value="Center" />
                            </Style>
                        </TabControl.Resources>
                        <!--Awards Tab-->
                        <TabItem x:Name="ScoreTab" Style="{StaticResource endGameText}" Margin="0,5,0,0">
                            <TabItem.Header>
                                <TextBlock Text="$$36174$$Awards" Style="{StaticResource CampaignTitleTextBlockStyle}" FontSize="25">
                                    <TextBlock.ToolTip>
                                        <TextBlock FontFamily="arial" FontSize="18" Text="$$19033$$View an overview of the scores in the game."/>
                                    </TextBlock.ToolTip>
                                </TextBlock>
                            </TabItem.Header>
                            <Grid Height="{Binding ElementName=ContainerGrid, Path=ActualHeight}">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="770"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>

                                <Grid.RowDefinitions>
                                    <RowDefinition Height="auto"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="290"/>
                                    <RowDefinition Height="75"/>
                                </Grid.RowDefinitions>

                                <!--Title-->
                                <TextBlock x:Name="TotalScore" Grid.Column="1" Grid.Row="0" Style="{StaticResource ConfirmationTitlesStyle}" Padding="5" FontSize="30" VerticalAlignment="Center" HorizontalAlignment="Stretch" TextAlignment="Center">
                                $$19060$$Total Score
                                </TextBlock>

                                <!--Most Resources/Least Resources/Most Improvements/most Military Units-->
                                <StackPanel Grid.Column="0" Grid.Row="1" Margin="5">
                                    <Grid>
                                        <Rectangle HorizontalAlignment="Left" VerticalAlignment="Center" Width="535" Height="140" Style="{StaticResource PanelBGSection}" />
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="15,0,0,0">
                                            <Image x:Name="uipostgameawardsdisplay_row0Icon" Height="100" Width="100" Source="{StaticResource postgame_awards_most_resources}" />

                                            <StackPanel HorizontalAlignment="Left" Margin="15,5,0,0">
                                                <TextBlock x:Name="uipostgameawardsdisplay_row0Award" Style="{StaticResource AwardsTitle}">$$31559$$Most Resources</TextBlock>
                                                <TextBlock x:Name="uipostgameawardsdisplay_row0Score" Style="{StaticResource endGameTextStyle1}" Foreground="Yellow">99,999</TextBlock>
                                                <TextBlock x:Name="uipostgameawardsdisplay_row0Player" Style="{StaticResource endGameTextStyle1}">Suleiman_the_Magnificent_Suleiman</TextBlock>
                                                <TextBlock x:Name="uipostgameawardsdisplay_row0XP" Style="{StaticResource endGameTextStyle1}" Foreground="Yellow">+ 999 xp</TextBlock>
                                            </StackPanel>
                                        </StackPanel>

                                        <!--Greyed out - Display when this reward is not achieved-->
                                        <Rectangle x:Name="uipostgameawardsdisplay_row0Overlay" HorizontalAlignment="Left" VerticalAlignment="Center" Width="535" Height="140" Style="{StaticResource PanelBGSection}" Visibility="Hidden"/>
                                    </Grid>

                                    <Grid Margin="0,12,0,0">
                                        <Rectangle HorizontalAlignment="Left" VerticalAlignment="Center" Width="535" Height="140" Style="{StaticResource PanelBGSection}" />
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="15,0,0,0">
                                            <Image x:Name="uipostgameawardsdisplay_row1Icon" Height="100" Width="100" Source="{StaticResource postgame_awards_least_resources}" />

                                            <StackPanel HorizontalAlignment="Left" Margin="15,5,0,0">
                                                <TextBlock x:Name="uipostgameawardsdisplay_row1Award" Style="{StaticResource AwardsTitle}">$$31560$$Least Resources</TextBlock>
                                                <TextBlock x:Name="uipostgameawardsdisplay_row1Score" Style="{StaticResource endGameTextStyle1}" Foreground="Yellow">99</TextBlock>
                                                <TextBlock x:Name="uipostgameawardsdisplay_row1Player" Style="{StaticResource endGameTextStyle1}">Suleiman the Magnificent</TextBlock>
                                                <TextBlock x:Name="uipostgameawardsdisplay_row1XP" Style="{StaticResource endGameTextStyle1}" Foreground="Yellow">+ 999 xp</TextBlock>
                                            </StackPanel>
                                        </StackPanel>

                                        <!--Greyed out - Display when this reward is not achieved-->
                                        <Rectangle x:Name="uipostgameawardsdisplay_row1Overlay" HorizontalAlignment="Left" VerticalAlignment="Center" Width="535" Height="140" Style="{StaticResource PanelBGSection}" Visibility="Hidden"/>
                                    </Grid>

                                    <Grid Margin="0,12,0,0">
                                        <Rectangle HorizontalAlignment="Left" VerticalAlignment="Center" Width="535" Height="140" Style="{StaticResource PanelBGSection}" />
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="15,0,0,0">
                                            <Image x:Name="uipostgameawardsdisplay_row2Icon" Height="100" Width="100" Source="{StaticResource postgame_awards_most_tech}" />

                                            <StackPanel HorizontalAlignment="Left" Margin="15,5,0,0">
                                                <TextBlock x:Name="uipostgameawardsdisplay_row2Award" Style="{StaticResource AwardsTitle}">$$31389$$Most Improvements</TextBlock>
                                                <TextBlock x:Name="uipostgameawardsdisplay_row2Score" Style="{StaticResource endGameTextStyle1}" Foreground="Yellow">99,999</TextBlock>
                                                <TextBlock x:Name="uipostgameawardsdisplay_row2Player" Style="{StaticResource endGameTextStyle1}">Suleiman the Magnificent</TextBlock>
                                                <TextBlock x:Name="uipostgameawardsdisplay_row2XP" Style="{StaticResource endGameTextStyle1}" Foreground="Yellow">+ 999 xp</TextBlock>
                                            </StackPanel>
                                        </StackPanel>

                                        <!--Greyed out - Display when this reward is not achieved-->
                                        <Rectangle x:Name="uipostgameawardsdisplay_row2Overlay" HorizontalAlignment="Left" VerticalAlignment="Center" Width="535" Height="140" Style="{StaticResource PanelBGSection}" Visibility="Hidden"/>
                                    </Grid>

                                    <Grid Margin="0,12,0,0">
                                        <Rectangle HorizontalAlignment="Left" VerticalAlignment="Center" Width="535" Height="140" Style="{StaticResource PanelBGSection}" />
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="15,0,0,0">
                                            <Image x:Name="uipostgameawardsdisplay_row3Icon" Height="100" Width="100" Source="{StaticResource postgame_awards_largest_army}" />

                                            <StackPanel HorizontalAlignment="Left" Margin="15,5,0,0">
                                                <TextBlock x:Name="uipostgameawardsdisplay_row3Award" Style="{StaticResource AwardsTitle}">$$31394$$Most Military Units</TextBlock>
                                                <TextBlock x:Name="uipostgameawardsdisplay_row3Score" Style="{StaticResource endGameTextStyle1}" Foreground="Yellow">99,999</TextBlock>
                                                <TextBlock x:Name="uipostgameawardsdisplay_row3Player" Style="{StaticResource endGameTextStyle1}">Suleiman the Magnificent</TextBlock>
                                                <TextBlock x:Name="uipostgameawardsdisplay_row3XP" Style="{StaticResource endGameTextStyle1}" Foreground="Yellow">+ 999 xp</TextBlock>
                                            </StackPanel>
                                        </StackPanel>

                                        <!--Greyed out - Display when this reward is not achieved-->
                                        <Rectangle x:Name="uipostgameawardsdisplay_row3Overlay" HorizontalAlignment="Left" VerticalAlignment="Center" Width="535" Height="140" Style="{StaticResource PanelBGSection}" Visibility="Hidden"/>
                                    </Grid>
                                </StackPanel>

                                <!--Most Unit Kills/Most Unit Lost/Highest Score/Most Treasures-->
                                <StackPanel Grid.Column="2" Grid.Row="1" Margin="5">
                                    <Grid>
                                        <Rectangle HorizontalAlignment="Left" VerticalAlignment="Center" Width="535" Height="140" Style="{StaticResource PanelBGSection}" />
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="15,0,0,0">
                                            <Image x:Name="uipostgameawardsdisplay_row4Icon" Height="100" Width="100" Source="{StaticResource postgame_awards_most_kills}" />

                                            <StackPanel HorizontalAlignment="Left" Margin="15,5,0,0">
                                                <TextBlock x:Name="uipostgameawardsdisplay_row4Award" Style="{StaticResource AwardsTitle}">$$31395$$Most Units Killed</TextBlock>
                                                <TextBlock x:Name="uipostgameawardsdisplay_row4Score" Style="{StaticResource endGameTextStyle1}" Foreground="Yellow">99,999</TextBlock>
                                                <TextBlock x:Name="uipostgameawardsdisplay_row4Player" Style="{StaticResource endGameTextStyle1}">Suleiman the Magnificent</TextBlock>
                                                <TextBlock x:Name="uipostgameawardsdisplay_row4XP" Style="{StaticResource endGameTextStyle1}" Foreground="Yellow">+ 999 xp</TextBlock>
                                            </StackPanel>
                                        </StackPanel>

                                        <!--Greyed out - Display when this reward is not achieved-->
                                        <Rectangle x:Name="uipostgameawardsdisplay_row4Overlay" HorizontalAlignment="Left" VerticalAlignment="Center" Width="535" Height="140" Style="{StaticResource PanelBGSection}" Visibility="Hidden"/>
                                    </Grid>

                                    <Grid Margin="0,12,0,0">
                                        <Rectangle HorizontalAlignment="Left" VerticalAlignment="Center" Width="535" Height="140" Style="{StaticResource PanelBGSection}" />
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="15,0,0,0">
                                            <Image x:Name="uipostgameawardsdisplay_row5Icon" Height="100" Width="100" Source="{StaticResource postgame_awards_most_losses}" />

                                            <StackPanel HorizontalAlignment="Left" Margin="15,5,0,0">
                                                <TextBlock x:Name="uipostgameawardsdisplay_row5Award" Style="{StaticResource AwardsTitle}">$$31457$$Most Units Lost</TextBlock>
                                                <TextBlock x:Name="uipostgameawardsdisplay_row5Score" Style="{StaticResource endGameTextStyle1}" Foreground="Yellow">99</TextBlock>
                                                <TextBlock x:Name="uipostgameawardsdisplay_row5Player" Style="{StaticResource endGameTextStyle1}">Suleiman the Magnificent</TextBlock>
                                                <TextBlock x:Name="uipostgameawardsdisplay_row5XP" Style="{StaticResource endGameTextStyle1}" Foreground="Yellow">+ 999 xp</TextBlock>
                                            </StackPanel>
                                        </StackPanel>

                                        <!--Greyed out - Display when this reward is not achieved-->
                                        <Rectangle x:Name="uipostgameawardsdisplay_row5Overlay" HorizontalAlignment="Left" VerticalAlignment="Center" Width="535" Height="140" Style="{StaticResource PanelBGSection}" Visibility="Hidden"/>
                                    </Grid>

                                    <Grid Margin="0,12,0,0">
                                        <Rectangle HorizontalAlignment="Left" VerticalAlignment="Center" Width="535" Height="140" Style="{StaticResource PanelBGSection}" />
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="15,0,0,0">
                                            <Image x:Name="uipostgameawardsdisplay_row6Icon" Height="100" Width="100" Source="{StaticResource postgame_awards_fame}" />
                                            <StackPanel HorizontalAlignment="Left" Margin="15,5,0,0">
                                                <TextBlock x:Name="uipostgameawardsdisplay_row6Award" Style="{StaticResource AwardsTitle}">$$32710$$Highest Score</TextBlock>
                                                <TextBlock x:Name="uipostgameawardsdisplay_row6Score" Style="{StaticResource endGameTextStyle1}" Foreground="Yellow">99</TextBlock>
                                                <TextBlock x:Name="uipostgameawardsdisplay_row6Player" Style="{StaticResource endGameTextStyle1}">Suleiman the Magnificent</TextBlock>
                                                <TextBlock x:Name="uipostgameawardsdisplay_row6XP" Style="{StaticResource endGameTextStyle1}" Foreground="Yellow">+ 999 xp</TextBlock>
                                            </StackPanel>
                                        </StackPanel>
                                        <!--Greyed out - Display when this reward is not achieved-->
                                        <Rectangle x:Name="uipostgameawardsdisplay_row6Overlay" HorizontalAlignment="Left" VerticalAlignment="Center" Width="535" Height="140" Style="{StaticResource PanelBGSection}" Visibility="Hidden"/>
                                    </Grid>

                                    <Grid Margin="0,12,0,0">
                                        <Rectangle HorizontalAlignment="Left" VerticalAlignment="Center" Width="535" Height="140" Style="{StaticResource PanelBGSection}" />
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="15,0,0,0">
                                            <Image x:Name="uipostgameawardsdisplay_row7Icon" Height="100" Width="100" Source="{StaticResource postgame_awards_most_treasures}" />

                                            <StackPanel HorizontalAlignment="Left" Margin="15,5,0,0">
                                                <TextBlock x:Name="uipostgameawardsdisplay_row7Award" Style="{StaticResource AwardsTitle}">$$31558$$Most Treasures</TextBlock>
                                                <TextBlock x:Name="uipostgameawardsdisplay_row7Score" Style="{StaticResource endGameTextStyle1}" Foreground="Yellow">99</TextBlock>
                                                <TextBlock x:Name="uipostgameawardsdisplay_row7Player" Style="{StaticResource endGameTextStyle1}">Suleiman the Magnificent</TextBlock>
                                                <TextBlock x:Name="uipostgameawardsdisplay_row7XP" Style="{StaticResource endGameTextStyle1}" Foreground="Yellow">+ 999 xp</TextBlock>
                                            </StackPanel>
                                        </StackPanel>
                                        <!--Greyed out - Display when this reward is not achieved-->
                                        <Rectangle x:Name="uipostgameawardsdisplay_row7Overlay" HorizontalAlignment="Left" VerticalAlignment="Center" Width="535" Height="140" Style="{StaticResource PanelBGSection}" Visibility="Hidden"/>
                                    </Grid>
                                </StackPanel>

